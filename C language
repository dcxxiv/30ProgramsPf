5

#include <stdio.h>
int main() {
    int a, b;
    printf("Enter first number: ");
    scanf("%d", &a);
    printf("Enter second number: ");
    scanf("%d", &b);

    printf("Sum = %d\n", a + b);
    printf("Product = %d", a * b);
    return 0;
}


