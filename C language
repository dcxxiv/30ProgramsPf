17

#include <stdio.h>
int main() {
    float num = 15.58971;
    int int_part;
    float frac_part;

    int_part = (int)num;
    frac_part = num - int_part;

    printf("Integer part = %d\n", int_part);
    printf("Fractional part = %.5f", frac_part);
    return 0;
}


18

#include <stdio.h>
int main() {
    float vi, t, a, s;
    scanf("%f %f %f", &vi, &t, &a);

    s = (vi * t) + (0.5 * a * t * t);

    printf("S = %.2f", s);
    return 0;
}


19

#include <stdio.h>
int main() {
    int age, months, days;
    scanf("%d", &age);

    months = age * 12;
    days = age * 365;

    printf("Months=%d\nDays=%d", months, days);
    return 0;
}


