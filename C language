22


#include <stdio.h>
int main() {
    char name[50], gender[10];
    int age;
    float height;

    printf("Enter name: ");
    scanf("%s", name);
    printf("Enter age: ");
    scanf("%d", &age);
    printf("Enter height: ");
    scanf("%f", &height);
    printf("Enter gender: ");
    scanf("%s", gender);

    printf("Name: %s\nAge: %d\nHeight: %.2f\nGender: %s", name, age, height, gender);
    return 0;
}


23

#include <stdio.h>
#define PI 3.1417

int main() {
    float r, area, cir;
    scanf("%f", &r);

    area = PI * r * r;
    cir = 2 * PI * r;

    printf("Area=%.2f\nCircumference=%.2f", area, cir);
    return 0;
}


24

#include <stdio.h>
int main() {
    int m1, m2, m3, m4, m5, total;
    float avg;

    scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5);

    total = m1 + m2 + m3 + m4 + m5;
    avg = total / 5.0;

    printf("Total=%d\nAverage=%.2f", total, avg);
    return 0;
}


25

#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c, s, area;
    scanf("%f %f %f", &a, &b, &c);

    s = (a + b + c) / 2;
    area = sqrt(s * (s - a) * (s - b) * (s - c));

    printf("Area=%.2f", area);
    return 0;
}


