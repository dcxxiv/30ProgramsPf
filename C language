    return 0;
}


27

#include <stdio.h>
int main() {
    int h, m, s, total;
    scanf("%d %d %d", &h, &m, &s);

    total = (h * 3600) + (m * 60) + s;

    printf("Total Seconds = %d", total);
    return 0;
}


28

#include <stdio.h>
int main() {
    float a, b, c, disc;
    scanf("%f %f %f", &a, &b, &c);

    disc = (b * b) - (4 * a * c);

    printf("Discriminant = %.2f", disc);
    return 0;
}


29

#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);

    if (n > 100) {
        printf("n is greater than 100");
    }
    return 0;
}


30

#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);

    if (n % 2 == 0)
        printf("Even");
    else
        printf("Odd");

    return 0;
}
