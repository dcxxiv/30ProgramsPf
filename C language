#include <stdio.h>
int main() {
    int age, months;
    printf("Enter age in years: ");
    scanf("%d", &age);

    months = age * 12;
    printf("Age in months = %d", months);
    return 0;
}


7


#include <stdio.h>
int main() {
    int pf, ic, cg, total;
    float average;
    float roll;

    printf("Enter Roll No: ");
    scanf("%f", &roll);
    printf("PF marks: ");
    scanf("%d", &pf);
    printf("IC marks: ");
    scanf("%d", &ic);
    printf("CG marks: ");
    scanf("%d", &cg);

    total = pf + ic + cg;
    average = total / 3.0;

    printf("Total = %d\n", total);
    printf("Average = %.2f", average);
    return 0;
}


8

#include <stdio.h>
int main() {
    float f, c;
    printf("Enter Fahrenheit: ");
    scanf("%f", &f);

    c = (5.0/9.0) * (f - 32);
    printf("Celsius = %.2f", c);
    return 0;
}


9

#include <stdio.h>
int main() {
    int a, b, c, d, max;
    scanf("%d %d %d %d", &a, &b, &c, &d);

    max = a;
    if (b > max) max = b;
    if (c > max) max = c;
    if (d > max) max = d;

    printf("Maximum = %d", max);
    return 0;
}


