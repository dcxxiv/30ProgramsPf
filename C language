    return 0;
}


12

#include <stdio.h>
int main() {
    const float PI = 3.1417;
    float r, h, volume;

    printf("Enter radius: ");
    scanf("%f", &r);
    printf("Enter height: ");
    scanf("%f", &h);

    volume = PI * r * r * h;
    printf("Volume = %.2f", volume);
    return 0;
}


13

#include <stdio.h>
#define PI 3.1417

int main() {
    float r, area;
    printf("Enter radius: ");
    scanf("%f", &r);

    area = PI * r * r;
    printf("Area = %.2f", area);
    return 0;
}


