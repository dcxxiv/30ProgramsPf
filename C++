#include <iostream>
using namespace std;
int main()
{
    float rupees = 12000;
    float dollorRate = 60;
    float dollors;
    
    dollors = rupees/dollorRate;
    cout<<"12000 rupees in dollors = " <<dollors<<endl;

    return 0;
}


27

#include <iostream>
using namespace std;
int main()
{
    int hour , minute , second ,totalTime;
    
    cout<<"Enter hour = ";
    cin>>hour;
    cout<<"Enter minute = ";
    cin>>minute;
    cout<<"Enter second = ";
    cin>>second;
    
    totalTime = hour*3600+minute*60+second;
    
    cout<<"Total time in seconds is =" <<totalTime<<endl;


    return 0;
}


