17

#include <iostream>
using namespace std;
int main()
{
    float num = 17.799943;
    int integral;
    float fractional;
    
    integral = int(num);
    fractional = num - integral;
    
    cout<<"original num is "<<num<<endl;
    cout<<"integral num is "<<integral<<endl;
    cout<<"fractional num is "<<fractional<<endl;
    
    return 0;
}


18

#include <iostream>
using namespace std;
int main()
{   int vi = 10;
    int t = 5;
    int a = 3;
    int s;
    s = vi * t + (0.5*a*t*t);
    cout<<"value of s is "<<s;

    return 0;
}

