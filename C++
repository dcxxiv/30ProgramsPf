#include <iostream>
using namespace std;
int main()
{
    int a, b, c, disc;
    cout<<"Enter a = ";
    cin>>a;
    cout<<"Enter b = ";
    cin>>b;
    cout<<"Enter c = ";
    cin>>c;
    
    disc =b*b-4*a*c;
    
    cout<<"Disc is = " <<disc;

    return 0;
}

