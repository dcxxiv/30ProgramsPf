#include<iostream>
using namespace std;
int main()
{   float celcius,farehnheit ;
    cout<<"Enter temp in farehnheit =";
    cin>>farehnheit;
    celcius = (farehnheit-32) *5.0/9.0;
    cout<<"temp in celcius = " <<celcius<<endl;

    return 0;
}

9

#include<iostream>
using namespace std;
int main()
{   int a, b, c, d, max;
    cout<<"Enter four numbers=";
    cin>> a>> b>> c>>d ;
    max = a;  //if a is maximum
    if(b>max)
      max=b;
    if(c>max)
      max=c;
    if(d>max)
      max=d;
    cout<<"maximum number is = "<<max <<endl;

    return 0;
}

