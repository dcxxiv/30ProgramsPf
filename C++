30

#include <iostream>
using namespace std;
int main()
{
    int n;
    
    cout<<"Enter value of n = ";
    cin>>n;
    
    if(n % 2 == 0)
      cout<<n<<" is even.";
    if(n % 2 != 0)
      cout<<n<<" is odd.";
    

    return 0;
}

