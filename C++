#include <iostream>
using namespace std;
int main()
{
    int a = 1 , b = 10 , c = 20;
    cout<<"a ="<<a<<endl;
    cout<<"b ="<<b<<endl;
    cout<<"c ="<<c<<endl;

    return 0;
}


