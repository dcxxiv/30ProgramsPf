#include <iostream>
const float PI = 3.1417;
using namespace std;
int main()
{   int radius , height ,volume ;    //Volume =Ï€R^2 xH 
    cout<<"Enter radius = ";
    cin>>radius;
    cout<<"Enter height = ";
    cin>>height;
    volume = PI*radius*radius*height;
    cout<<"Volume is = "<<volume;

    return 0;
}

