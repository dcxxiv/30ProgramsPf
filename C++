7

#include <iostream>
using namespace std;
int main()
{   int rollNo = 239;
    float PF ,calculus , AI;
    float  total ,average;
    cout<<"Enter rollNo = ";
    cin >> rollNo;
    cout<<"Enter marks in PF = ";
    cin >> PF;
    cout<<"Enter marks in calculus = ";
    cin >> calculus;
    cout<<"Enter marks in AI = ";
    cin >> AI;
    total = PF + calculus + AI;
    average = total/3;
    cout<<"rollNO = " << rollNo << endl;
    cout<<"average marks = " << average << endl;
    cout<<"total marks = " << total << endl;
    
    return 0;
}

