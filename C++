3

#include <iostream>
using namespace std;
int main()
{   int a = 1 , b = 2,temp;
    temp = a;
    a = b;
    b = temp;
    cout<<"values after interchanging:"<<endl;
    cout<<"a = "<< a << ", b = " << b << endl;
    return 0;
}
